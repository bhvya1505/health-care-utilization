PATIENT
resource.resourceType = "Patient"   +
resource.id (Patient id)    +
resource.extension - 4 elements
    [0] - url - "*/patient-mothersMaidenName"; valueString +
    [1] - url - "*/patient-birthPlace"; valueAddress.city, valueAddress.state, valueAddress.country
    [2] - url - "*/disability-adjusted-life-years"; valueDecimal
        [3] - url - "*/quality-adjusted-life-years"; valueDecimal
resource.identifier - 3 elements
    [1] - (Medical Record Number)
        type.coding.system, type.coding.code = "MR", type.coding.display = "Medical Record Number"
        system
        value
    [2] - (SSN)
        type.coding.system, type.coding.code = "SS", type.coding.display = "Social Security Number"
        system
        value
resource.name - array (pick only the official name)     +
    use - "official"
    family
    given - array
resource.telecom - array +
    system - phone
    value
    use
resource.gender     +
resource.birthdate      +
resource.address - array    +
    extension - array
        extension.url - "*/geolocation"
        extension.extension - array
            [0] - url = "latitude"; valueDecimal
            [1] - url = "longitude"; valueDecimal
    line - array (combine into single line)
    city
    state
    postalcode
    country
resource.maritalStatus  +
    coding - array
        system, display = "Never Married"
        code
    text = "Never Married"
resource.multipleBirthBoolean
resource.communication - array
    language.coding.system, language.coding.code, language.coding.display
    language.text
