ENCOUNTER
resource.resourceType = "Encounter" +
resource.id (encounter id)  +
resource.status +
resource.class +
    system, code
resource.type - array   +
    coding.system, coding.code, coding.display
    text
resource.subject    +
    reference (urn:uuid:patient id), display (patient name)
resource.participant - array (practitioners)    +
    type.coding.system, type.coding.code, type.coding.display (type - array)
    period.start, period.end
    individual.reference, individual.name
resource.period     +
    start, end
resource.serviceProvider    +
    reference, display

